@{
    #pragma warning disable CS8602 // Disable nullable warning
}

@model IEnumerable<string>
    <!-- Define the model as a collection of strings -->

    @{
    Layout = null;  // Set Layout to null, meaning no layout page is used
    }

    <!DOCTYPE html>
    <html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>Language Features</title>  <!-- Set the page title -->
    </head>
    <body>
        <ul>
            @* Ensure the `foreach` loop is properly wrapped in curly braces *@
            @foreach (var s in Model)
        {
            <li>@s</li>
  <!-- Display each string in a list item -->
        }
        </ul>
    </body>
</html>
